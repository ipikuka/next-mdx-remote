{
  "name": "@ipikuka/next-mdx-remote",
  "version": "1.0.0-alpha.10",
  "description": "It is an opinionated wrapper for the serialize function of the `next-mdx-remote` package that written by hashicorp",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js"
    }
  },
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint \"{**/*,*}.{js,ts}\"",
    "prettier": "prettier --write \"{src,tests,example/src}/**/*.{js,ts}\"",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --config ./jest.config.cjs",
    "test:file": "NODE_OPTIONS=--experimental-vm-modules jest --config ./jest.config.cjs serialize.plugin.test.tsx",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run prettier && npm run lint",
    "prebuild": "rimraf dist"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "global.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ipikuka/next-mdx-remote.git"
  },
  "keywords": [
    "mdx",
    "markdown",
    "next",
    "nextjs",
    "next.js",
    "next-mdx-remote",
    "remark",
    "rehype"
  ],
  "author": "ipikuka <talatkuyuk@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/ipikuka/next-mdx-remote#readme",
  "bugs": {
    "url": "https://github.com/ipikuka/next-mdx-remote/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "github-slugger": "^2.0.0",
    "gray-matter": "^4.0.3",
    "hast-util-has-property": "^2.0.1",
    "hast-util-to-string": "^2.0.0",
    "hastscript": "^7.2.0",
    "mdast-util-to-string": "^3.1.1",
    "next-mdx-remote": ">=4.x",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-prism-plus": "^1.5.1",
    "rehype-slug": "^5.1.0",
    "remark": "^14.0.2",
    "remark-breaks": "^3.0.2",
    "remark-definition-list": "^1.2.0",
    "remark-emoji": "^3.1.0",
    "remark-fix-guillemets": "^1.1.2",
    "remark-flexible-code-titles": "^1.1.1",
    "remark-flexible-containers": "^1.0.2",
    "remark-flexible-paragraphs": "^1.0.0",
    "remark-frontmatter": "^4.0.1",
    "remark-gemoji": "^7.0.1",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "remark-smartypants": "^2.0.0",
    "remark-stringify": "^10.0.2",
    "remark-supersub": "^1.0.0",
    "remark-textr": "^5.0.1",
    "unist-util-visit": "^4.1.2"
  },
  "devDependencies": {
    "@types/dedent": "^0.7.0",
    "@types/hast": "^2.3.4",
    "@types/jest": "^29.4.0",
    "@types/mdast": "^3.0.10",
    "@types/node": "^18.14.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/unist": "^2.0.6",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "dedent": "^0.7.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.4.3",
    "prettier": "^2.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5",
    "unified": "^10.1.2",
    "vfile": "^5.3.7"
  },
  "peerDependencies": {
    "next-mdx-remote": ">=4.x",
    "react": ">=16.x <=18.x",
    "react-dom": ">=16.x <=18.x"
  }
}
